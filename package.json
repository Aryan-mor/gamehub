{
  "name": "gamehub",
  "version": "0.1.0",
  "private": true,
  "main": "dist/bot.js",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "build": "tsc",
    "start": "node dist/bot.js",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 200",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter=verbose",
    "test:scenarios": "vitest run tests/scenarios/",
    "test:scenarios:ui": "vitest tests/scenarios/ --ui",
    "test:scenarios:coverage": "vitest run tests/scenarios/ --coverage",
    "test:scenarios:ci": "vitest run tests/scenarios/ --reporter=verbose",
    "pre-push:full": "./scripts/pre-push.sh",
    "pre-push:coverage": "./scripts/pre-push.sh --coverage",
    "bot": "tsx src/bot.ts",
    "bot:stop": "pkill -f 'pnpm bot' || pkill -f 'tsx src/bot.ts' || echo 'No bot processes found'",
    "bot:restart": "pnpm run bot:stop && sleep 2 && pnpm run bot",
    "bot:manage": "tsx src/scripts/botManager.ts",
    "bot:control": "./scripts/bot-control.sh",
    "export:bot-config": "ts-node src/scripts/exportBotConfig.ts",
    "set:bot-commands": "ts-node src/scripts/setBotCommands.ts",
    "type-check": "tsc --noEmit",
    "migrate:supabase": "tsx scripts/migrate-to-supabase.ts",
    "test:supabase": "tsx scripts/test-supabase-connection.ts",
    "fix:chat-id": "tsx scripts/fix-chat-id.ts",
    "fix:chat-id-simple": "tsx scripts/fix-chat-id-simple.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.4.5",
    "grammy": "^1.37.0",
    "i18next": "^25.3.2",
    "i18next-fs-backend": "^2.6.0",
    "nanoid": "^5.1.5",
    "node-fetch": "2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "form-data": "^4.0.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "tough-cookie": "^4.1.3",
    "request": "^2.88.2",
    "form-data": "^4.0.4"
  }
}
